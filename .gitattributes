# Auto detect text files and perform LF normalization
*        text=auto

*.cs     text diff=csharp
*.java   text diff=java
*.html   text diff=html
*.css    text eol=lf
*.scss   text eol=lf
*.js     text eol=lf
*.json   text eol=lf
*.xml    text eol=lf
*.php    text eol=lf
*.rb     text eol=lf
*.md     text eol=lf
*.log    text eol=lf
*.sql    text

*.csproj text merge=union
*.sln    text merge=union eol=crlf

*.docx   diff=astextplain
*.DOCX   diff=astextplain